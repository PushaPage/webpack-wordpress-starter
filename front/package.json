{
    "private": true,
    "engines": {
        "node": ">=16.0.0",
        "yarn": "please-use-npm",
        "npm": ">=7.10.0"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --config config/webpack.dev.config.js --mode=development",
        "start": "cross-env NODE_ENV=development webpack --config config/webpack.dev.config.js --watch --mode=development",
        "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js --mode=production",
        "stats": "webpack --config config/webpack.prod.config.js --mode=production --json > stats.json && webpack-bundle-analyzer stats.json",
        "lint:js": "eslint --ext .js src/js/",
        "lint:css": "stylelint \"src/styles/**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,json,html,css}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,json,html,css}\"",
        "prepare": "cd .. && husky install front/.husky"
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "browser-sync": "^2.27.11",
        "browser-sync-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "eslint": "^8.16.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.26.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.1",
        "imagemin-webpack-plugin": "^2.4.2",
        "mini-css-extract-plugin": "^2.6.0",
        "node-sass": "^8.0.0",
        "portfinder-sync": "^0.0.2",
        "postcss": "^8.4.14",
        "postcss-loader": "^7.0.0",
        "prettier": "^2.6.2",
        "sass-loader": "^13.2.0",
        "stylelint": "^14.16.0",
        "stylelint-config-prettier": "^9.0.4",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-config-standard-scss": "^6.1.0",
        "stylelint-scss": "^4.3.0",
        "terser-webpack-plugin": "^5.3.1",
        "webpack": "^5.72.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-manifest-plugin": "^5.0.0",
        "webpack-merge": "^5.8.0"
    },
    "browserslist": [
        ">=1%",
        "not dead",
        "not op_mini all",
        "not and_uc 12",
        "Firefox >= 80",
        "Chrome >= 80",
        "Opera >= 70",
        "Safari >= 14",
        "Edge >= 80",
        "ios_saf >= 14",
        "Samsung >= 11",
        "and_chr >= 80",
        "and_ff >= 80"
    ]
}
